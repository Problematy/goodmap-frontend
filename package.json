{
    "name": "@problematy/goodmap",
    "version": "0.2.1",
    "engines": {
        "node": ">=16.0.0"
    },
    "description": "Fronted scripts for map",
    "main": "dist/index.js",
    "scripts": {
        "build": "webpack --mode production --progress --config webpack.config.js",
        "dev-build": "webpack --progress --mode development --config webpack.config.js",
        "watch": "webpack --progress -d --config webpack.config.js --watch",
        "serve": "webpack-dev-server --mode development",
        "test": "jest --config jest.config.js",
        "coverage": "jest --coverage --config jest.config.js",
        "lint-fix": "eslint src/**/*.jsx tests/**/*.jsx --fix",
        "lint": "eslint src/**/*.jsx tests/**/*.jsx"
    },
    "files": [
        "dist"
    ],
    "jest": {
        "transform": {
            "^.+\\.(js|jsx)$": "babel-jest"
        }
    },
    "license": "MIT",
    "devDependencies": {
        "@babel/core": "^7.18.5",
        "@babel/eslint-parser": "^7.19.1",
        "@babel/preset-env": "^7.18.2",
        "@babel/preset-react": "^7.17.12",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^16.0.1",
        "@types/leaflet": "^1.9.0",
        "babel-jest": "^28.1.1",
        "babel-loader": "^8.2.5",
        "css-loader": "^6.8.1",
        "eslint": "^8.30.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.31.11",
        "eslint-plugin-react-hooks": "^4.6.0",
        "jest": "^28.1.1",
        "jest-environment-jsdom": "^29.3.1",
        "prettier": "^2.8.1",
        "style-loader": "^3.3.3",
        "terser-webpack-plugin": "^5.3.6",
        "url-loader": "^4.1.1",
        "webpack": "^5.73.0",
        "webpack-cli": "^4.10.0",
        "webpack-dev-server": "^4.9.3"
    },
    "dependencies": {
        "@emotion/react": "^11.11.1",
        "@emotion/styled": "^11.11.0",
        "@mui/icons-material": "^5.14.6",
        "@mui/material": "^5.14.6",
        "leaflet": "^1.9.3",
        "leaflet.markercluster": "^1.5.3",
        "npm": "^10.2.4",
        "prop-types": "^15.8.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-leaflet": "^4.2.1",
        "react-leaflet-cluster": "^2.1.0",
        "react-leaflet-custom-control": "^1.3.5"
